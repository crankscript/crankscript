{
    "name": "@crankscript/test",
    "version": "0.0.0",
    "author": "Alberto Piu",
    "license": "MIT",
    "type": "commonjs",
    "description": "The CrankScript testing library",
    "scripts": {
        "prebuild": "tsx --tsconfig ../cli/tsconfig.json ../cli/src/index.ts transpile --input ./src/import.ts",
        "build": "mkdir -p ../../dist/libs/test && cp -r ./Source ../../dist/libs/test/Source && cp ./package.json ../../dist/libs/test/package.json"
    },
    "dependencies": {
        "@crankscript/core": "0.3.0",
        "@crankscript/types": "1.1.0",
        "crankscript": "0.17.0"
    },
    "peerDependencies": {
        "lua-types": "*"
    },
    "types": "./Source/index.d.ts",
    "main": "./Source/index"
}
